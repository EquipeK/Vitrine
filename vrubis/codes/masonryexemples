  <script src="//ajax.googleapis.com/ajax/libs/jquery/2.0.3/jquery.min.js"></script>
  <script src="/vendor/jquery-ias/dist/jquery-ias.min.js" type="text/javascript"></script>
  <script src="/vendor/masonry/dist/masonry.pkgd.min.js" type="text/javascript"></script>
              
  <script type="text/javascript">
    var container = document.querySelector('.container');
    var msnry = new Masonry( container, {
      // options
      itemSelector: '.item',
      gutter: 10
    });

    var ias = $.ias({
      container: ".container",
      item: ".item",
      pagination: "#pagination",
      next: ".next a",
      delay: 1200
    });

    ias.on('render', function(items) {
      $(items).css({ opacity: 0 });
    });

    ias.on('rendered', function(items) {
      msnry.appended(items);
    });

    ias.extension(new IASSpinnerExtension());
    ias.extension(new IASNoneLeftExtension({html: '&lt;div class="ias-noneleft" style="text-align:center"&gt;&lt;p&gt;&lt;em&gt;You reached the end!&lt;/em&gt;&lt;/p&gt;&lt;/div&gt;'}));
  </script>
  </div>
